{
  "name": "optimizely/optimizely-sdk",
  "description": "Optimizely PHP SDK for Optimizely Feature Experimentation, Optimizely Full Stack (legacy), and Optimizely Rollouts",
  "keywords": ["optimizely", "sdk"],
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Optimizely",
      "email": "developers@optimizely.com"
    }
  ],
  "scripts": {
        "test": "phpunit",
        "lint": "phpcs",
        "beautify": "phpcbf"
  },
  "require": {
    "php": ">=8.1",
    "guzzlehttp/guzzle": ">=6.2",
    "justinrainbow/json-schema": "^1.6 || ^2.0 || ^4.0 || ^5.0",
    "lastguest/murmurhash": "^1.3.0",
    "monolog/monolog": ">=1.21"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3",
    "php-coveralls/php-coveralls": "v2.5.3",
    "squizlabs/php_codesniffer": "3.7",
    "icecave/parity": "^3.0.1"
  },
  "autoload": {
    "psr-4": {
      "Optimizely\\": "src/Optimizely"
    }
  },
  "config": {
    "sort-packages": true
  }
}
